@{
    ViewData["Title"] = "Internship";
}
@section scripts {
    <script type="text/javascript" src="~/js/Index.js"></script>
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Internship Landpage</title>
</head>
<body>
    <div>
        <h1>@ViewData["Title"]</h1>
        <p>This is our classroom.</p>
        <ul id="list">
            @for (var i = 0; i < Model.Members.Count; i++)
            {
                var member = Model.Members[i];

                <li class="member">
                    @member
                </li>
            }
        </ul>
    </div>

    <input id="newcomer" title="Name" type="text" />
    <button id="add" class="btn btn-primary">Add new commer</button>
    <button id="clear" class="btn btn-secondary"> Clear</button>
    <p><img src="/Images/Rules.jpg" width="200" /></p>
</body>
</html>
